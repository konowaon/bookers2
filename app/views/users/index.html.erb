<%= render 'list', user: @user %>
        
<!-- form_with部分 -->
<%= form_with model: Book.new do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
	
  <h4>Opinion</h4>
  <%= f.text_area :body %>
	
  <%= f.submit 'Creat Book' %>
<% end %>
<main>
      </div>
      <div class='col-md-8 offset-md-1'>
        <h2>Users</h2>
        <!-- 投稿した書籍一覧表示を作成しましょう。 -->
        <table class='table table-hover table-inverse'>
          <thead>
            <tr>
              <th></th>
              <th>image</th>
              <%= image_tag 'default-image.jpg' %>
              <th>name</th>
              <td><%= current_user.name %></td>
              <td><%= link_to "show" %></td>
              <th colspan="3"></th>
            </tr>
          </thead>
          <tbody></tbody>
        </table>
      </div>
    </div>
  </div>
</main>